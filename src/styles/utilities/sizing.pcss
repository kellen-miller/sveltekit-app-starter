/* Size utility */
.w-100 {
    width: 100%;
}

.w-auto {
    width: auto;
}

.vw-50 {
    width: 50vw;
}

.h-100 {
    height: 100%;
}

/* end of size utility */


/* Fixed size utility */
.f-w-5 {
    width: 1.25rem;
}

.f-w-6 {
    width: 1.5rem;
}

.f-h-5 {
    height: 1.25rem;
}

.f-h-12 {
    height: 3rem;
}

/* end of fixed size utility */